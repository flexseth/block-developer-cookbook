{
	"name": "block-developers-cookbook",
	"version": "0.1.0",
	"description": "Example block scaffolded with Create Block tool.",
	"author": "Ryan Welcher",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"wp-now": "wp-now",
		"block": "cd ./plugins && npx @wordpress/create-block --template ../templates/bdc --variant dynamic",
		"plugin": "cd ./plugins && npx @wordpress/create-block --template ../templates/bdc --variant plugin",
		"build": "pnpm -r build",
		"start": "pnpm -r start"
	},
	"devDependencies": {
		"@wordpress/scripts": "^26.11.0",
		"@wp-now/wp-now": "^0.1.61"
	},
	"prettier": "@wordpress/prettier-config"
}
